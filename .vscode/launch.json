    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Gpt File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "txt_gpt.env",
                "-g",
                "RTX-PRO-6000_1"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src" // Replace with your source folder path
            }
        }
    ]
}